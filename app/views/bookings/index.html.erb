
<h1>My tickets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Address</th>
      <th>Tickets</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.gig.name %></td>
        <td><%= booking.gig.date %></td>
        <td><%= booking.gig.address.full_address %></td>
        <td><%= booking.tickets %></td>
        <td><%= booking.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


